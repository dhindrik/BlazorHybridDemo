<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:components="clr-namespace:CatchLoggerDemo.Core.Components;assembly=CatchLoggerDemo.Core"
             x:Class="CatchLoggerDemo.Apps.RegisterPage"
             Title="RegisterPage" Padding="0">
    <StackLayout BackgroundColor="#CCCCCC">
        <ContentView BackgroundColor="#FFF">
     <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html" HeightRequest="400" WidthRequest="300" BackgroundColor="Transparent">
        <BlazorWebView.RootComponents>
            <RootComponent x:Name="Component" Selector="#app" ComponentType="{x:Type components:RegisterForm}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
            </ContentView>
    </StackLayout>
</ContentPage>
